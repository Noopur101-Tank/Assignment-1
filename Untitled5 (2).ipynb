   "source": [
    "def fibonacci_series(num):\n",
    "    series=[]\n",
    "    a,b=0,1\n",
    "    \n",
    "    while b<=num:\n",
    "        series.append(b)\n",
    "        a,b=b,a+b\n",
    "    return series\n",
    "fibo_numbers=fibonacci_series(50)\n",
    "print(fibo_numbers)"
   ]
  }
